{
  "name": "home",
  "version": "2.0.0",
  "description": "home",
  "main": "./server/index.js",
  "scripts": {
    "start": "node ./server/index.js",
    "dev": "NODE_ENV=development nodemon -x 'node --inspect server/index.js'",
    "test": "jest",
    "test:watch": "jest --watch",
    "main:build": "elm-static-html -f ./client/main/Main.elm -o ./public/index.html && sed -i '1s/^/<!DOCTYPE html>\\n/' ./public/index.html",
    "main:watch": "nodemon -e ./client/main/Main.elm -x 'npm run main:build'",
    "pwa:build": "NODE_ENV=production webpack",
    "pwa:watch": "webpack --watch",
    "heroku-postbuild": "npm i -g elm && npm run main:build && npm run pwa:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ronanyeah/home.git"
  },
  "author": "ronanyeah",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ronanyeah/home/issues"
  },
  "homepage": "https://ronanmccabe.me/",
  "dependencies": {
    "asn1.js": "^5.0.0",
    "chalk": "^2.3.0",
    "elm-static-html": "^1.3.2",
    "elm-webpack-loader": "^4.4.0",
    "fluture": "^7.2.2",
    "highland": "^2.10.1",
    "http_ece": "^1.0.5",
    "joi": "^13.0.2",
    "jws": "^3.1.4",
    "newrelic": "^2.4.2",
    "node-fetch": "^1.7.3",
    "ramda": "^0.25.0",
    "redis": "^2.8.0",
    "rotools": "^1.3.0",
    "urlsafe-base64": "^1.0.0",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "jest": "^22.0.4",
    "nodemon": "^1.14.3"
  },
  "engines": {
    "node": "8.3.0"
  }
}
