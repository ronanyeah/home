{
  "name": "home",
  "version": "2.0.0",
  "description": "home",
  "main": "./server/index.js",
  "scripts": {
    "start": "node ./server/index.js",
    "test": "node ./test/index.js | tap-spec",
    "watch:test": "nodemon --ignore test/scrap -x 'npm test'",
    "watch:server": "NODE_ENV=development nodemon -x 'node --inspect server/index.js'",
    "build": "node ./client/main/BUILD.js",
    "postinstall": "npm run build",
    "watch:main": "nodemon -e css -e html -x 'npm run build:main'",
    "build:toys": "elm-make ./client/toys/Main.elm --warn --output ./public/toys/bundle.js",
    "watch:toys": "nodemon -e elm -x 'npm run build:toys'",
    "cov": "istanbul cover ./test/index.js",
    "pwa": "ngrok http 3000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ronanyeah/home.git"
  },
  "author": "ronanyeah",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ronanyeah/home/issues"
  },
  "homepage": "https://ronanmccabe.me/",
  "dependencies": {
    "autoprefixer": "^6.5.1",
    "chalk": "^1.1.3",
    "cssnano": "^3.8.0",
    "fluture": "^5.0.0",
    "fs-extra": "^2.1.2",
    "highland": "^2.10.1",
    "http_ece": "^0.7.2",
    "joi": "^10.0.6",
    "jws": "^3.1.4",
    "newrelic": "^1.38.2",
    "node-fetch": "^1.6.3",
    "postcss-import": "^9.0.0",
    "precss": "^1.4.0",
    "ramda": "^0.23.0",
    "redis": "^2.7.1",
    "rotools": "^1.1.1",
    "urlsafe-base64": "^1.0.0",
    "webpack": "^2.3.3",
    "wordcloud": "^1.0.5"
  },
  "devDependencies": {
    "dotenv": "^4.0.0",
    "istanbul": "^0.4.5",
    "nock": "^9.0.2",
    "nodemon": "^1.11.0",
    "shot": "^3.4.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2"
  },
  "engines": {
    "node": "6.10.2"
  }
}
